{
  "name": "wcinyp-radiology-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ci": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate:design": "node scripts/validate-design-system.js",
    "test:e2e": "playwright test",
    "test:fumadocs": "playwright test tests/e2e/fumadocs",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test tests/e2e/visual",
    "test:visual:update": "playwright test tests/e2e/visual --grep 'update all baselines'",
    "test:perf": "playwright test --project=performance-desktop tests/e2e/performance",
    "test:perf:mobile": "playwright test --project=performance-mobile tests/e2e/performance",
    "test:perf:slow": "playwright test --project=performance-slow-network tests/e2e/performance",
    "test:perf:all": "playwright test tests/e2e/performance/*.perf.spec.ts",
    "test:perf:debug": "PWDEBUG=1 playwright test --project=performance-desktop tests/e2e/performance",
    "test:perf:trace": "playwright test --project=performance-desktop tests/e2e/performance --trace on",
    "test:perf:report": "playwright show-report test-results/html-report",
    "test:performance": "./scripts/run-performance-tests.sh",
    "test:performance:ci": "./scripts/performance-ci.sh",
    "perf:generate-report": "tsx tests/e2e/performance/generate-report.ts",
    "perf:generate-report:trends": "tsx tests/e2e/performance/generate-report.ts --trends",
    "perf:update-baseline": "tsx tests/e2e/performance/generate-report.ts --baseline",
    "perf:baseline": "./scripts/update-performance-baseline.sh",
    "perf:check": "./scripts/run-performance-tests.sh --profile all --no-report",
    "test:parallel": "./scripts/parallel-test-runner.sh",
    "test:flaky": "node scripts/detect-flaky-tests.js",
    "test:ci:fast": "CI=true npm run test:parallel",
    "test:health": "./scripts/daily-test-health.sh",
    "test:report:weekly": "node scripts/weekly-test-report.js",
    "test:dashboard": "node scripts/generate-test-dashboard.js && open test-results/test-dashboard.html"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@netlify/functions": "^4.1.10",
    "@niivue/niivue": "^0.61.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
